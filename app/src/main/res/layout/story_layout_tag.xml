<?xml version="1.0" encoding="utf-8"?>
<co.lujun.androidtagview.TagContainerLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:id="@+id/tag_container"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginLeft="10dp"
    android:layout_marginStart="10dp"
    android:layout_marginEnd="@dimen/activity_horizontal_margin"
    android:layout_marginRight="@dimen/activity_horizontal_margin"
    android:padding="10dp"
    app:container_enable_drag="false"
    app:container_background_color="#00000000"
    app:container_border_color="#00000000"
    app:tag_text_color="@color/defaultText"
    app:tag_border_color="@color/defaultText"
    app:tag_background_color="#00000000"
    app:horizontal_interval="10dp"
    app:tag_horizontal_padding="16dp"
    app:tag_vertical_padding="10dp"
    app:tag_clickable="true"
    app:tag_theme="none"
    app:vertical_interval="10dp" />